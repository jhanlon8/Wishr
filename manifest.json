{
    "name": "Wishr",
    "version": "0.01",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://*.amazon.com/*", "https://*.walmart.com/*", "https://*.target.com/*"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Wishr"
    }
}
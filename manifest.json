{
    "name": "Wishr",
    "version": "0.01",
    "manifest_version": 2,
    "background": {
        "scripts": ["./background.js"]
    },
    "browser_action": {
        "default_popup": "mainPage.html",
        "default_title": "Wishr"
    },
    "permissions": [
        "storage",
        "http://localhost:3000/"
    ]
}
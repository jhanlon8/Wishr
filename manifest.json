{
    "name": "Wishr",
    "version": "0.01",
    "manifest_version": 2,
    "background": {
        "scripts": ["./background.js"]
    },
    "browser_action": {
        "default_popup": "login.html",
        "default_title": "Wishr"
    },
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "https://wishr.loca.lt/"
    ]
}
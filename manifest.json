{
    "name": "Wishr",
    "version": "1.0",
    "manifest_version": 2,
    "description": "A platform agnostic wishlist extension!",
    "background": {
        "scripts": ["./background.js"]
    },
    "browser_action": {
        "default_popup": "login.html",
        "default_title": "Wishr",
        "default_icon": "images/wishIcon-16x16.png"
    },
    "permissions": [
        "storage",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "https://wishr.loca.lt/",
        "https://*.amazon.com/",
        "https://*.walmart.com/",
        "https://*.target.com/",
        "https://*.ebay.com/",
        "https://*.etsy.com/"
    ]
}